{
  "name": "postcss-less",
  "version": "2.0.0",
  "description": "LESS parser for PostCSS",
  "license": "MIT",
  "repository": "webschik/postcss-less",
  "keywords": [
    "css",
    "postcss",
    "postcss-syntax",
    "parser",
    "less"
  ],
  "main": "dist/less-syntax.js",
  "files": [
    "dist"
  ],
  "author": "Denys Kniazevych <webschik@gmail.com>",
  "maintainer": {
    "name": "Andrew Powell",
    "email": "andrew@shellscape.org",
    "url": "shellscape.org"
  },
  "engines": {
    "node": ">=6.14.4"
  },
  "scripts": {
    "ci:coverage": "echo 'no coverage'",
    "ci:lint": "npm run lint && npm run security",
    "ci:lint:commits": "commitlint --from=${CIRCLE_BRANCH} --to=${CIRCLE_SHA1}",
    "ci:test": "npm run test -- --runInBand",
    "commitlint": "commitlint",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "tslint --project . --fix 'src/**/*.ts' && eslint --cache .",
    "lint-staged": "lint-staged",
    "security": "npm audit",
    "test": "ava"
  },
  "dependencies": {
    "postcss": "^5.2.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "ava": "^1.0.0-beta.8",
    "eslint": "^5.6.0",
    "eslint-config-shellscape": "^1.0.0",
    "eslint-plugin-filenames": "^1.2.0",
    "less": "^2.6.1",
    "lint-staged": "^7.2.2",
    "postcss-parser-tests": "^5.0.11",
    "pre-commit": "^1.2.2",
    "standard-version": "^4.4.0"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
